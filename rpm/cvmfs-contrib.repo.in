[cvmfs-contrib]
name=CernVM Filesystem Contributions
type=rpm-md
# browsable alternate at http://download.opensuse.org/repositories/home:/cvmfs:/contrib/{distro}_{rhel}/
baseurl=http://downloadcontent.opensuse.org/repositories/home:/cvmfs:/contrib/{distro}_{rhel}/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CVMFS-CONTRIB
enabled=1

